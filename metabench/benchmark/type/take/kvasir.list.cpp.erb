#include <kvasir/mpl/mpl.hpp>
namespace mpl = kvasir::mpl;

template<int> struct x;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mpl::list<<%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::eager::take<<%= "xs#{k}" %>, <%= k*n/env[:k] %>>;
#endif
<% end %>

int main() {}
