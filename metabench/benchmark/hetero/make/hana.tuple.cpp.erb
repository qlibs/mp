#include <boost/hana/tuple.hpp>
namespace hana = boost::hana;

template<int> struct x {};

int main() {
<% env[:k].times do |k| %>
#if defined(METABENCH)
    constexpr auto <%= "xs#{k}" %> = hana::make_tuple(
        <%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>{}" }.join(', ') %>
    );
#endif
<% end %>
}
